+
+		soc_pcm_hw_update_rate(hw, stream);
+		soc_pcm_hw_update_chan(hw, stream);
+		soc_pcm_hw_update_format(hw, stream);
 	}
 
 	dpcm_runtime_merge_format(substream, runtime);
